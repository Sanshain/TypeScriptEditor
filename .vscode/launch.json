    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch rollup NPM",
            "args": [
                "-c"
            ],
            "program": "${workspaceFolder}/node_modules/rollup/dist/bin/rollup",            
            // "runtimeExecutable": "npm",
            // "runtimeArgs": [
            //     "run-script",
            //     "debug"
            // ],
            // "port": 9229,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            // "program": "${workspaceFolder}\\gulpfile.js",
            "args": [
                "build"
            ],   
            "program": "${workspaceFolder}\\node_modules\\gulp\\bin\\gulp.js",            
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}